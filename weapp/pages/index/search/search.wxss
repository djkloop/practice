.container {
  padding: 20rpx 30rpx;
}

.search-input {
  position: relative;
  display: flex;
  align-items: center;
  height: 70rpx;
}

.search-input input {
  flex: 5;
  padding-left: 80rpx;
  height: 100%;
  font-size: 32rpx;
  border-radius: 20rpx;
  background: #f5f5f5;
}

.search-input icon {
  position: absolute;
  top: 16prx;
  left: 16rpx;
  z-index: 9;
}

.search-input navigator {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
}